{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "cors": true
    },
    "android": {
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "INTERNET"
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "name": "PayeTonKawa",
    "slug": "PayeTonKawa",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ]

  }
}
